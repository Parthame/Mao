// din ko saal me badalta hain or saal ko hafte me hafto ko dino me
    #include<iostream>
    using namespace std;
    
    int main()
    {
        int y,d,w;
        cout<<"Din dallana to:)";
        cin>>d;
        
        y = d/365;
        d = d%365;
        w = d/7;
        d = d%7;
        
        cout << "no. years:" << y << endl << "no. of weeks:" << w  << endl << "no. of days:" << d;
        
        return 0;
    }
